{
  "name": "sql-test-3",
  "version": "1.0.0",
  "description": "SQL Database Testing Project with Docker and PostgreSQL",
  "main": "test.js",
  "scripts": {
    "test": "node test.js",
    "validate": "node validate.js",
    "demo": "node demo.js",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "logs": "docker-compose logs postgres",
    "reset": "docker-compose down -v && docker-compose up -d",
    "setup": "./setup.sh",
    "status": "docker-compose ps",
    "connect": "docker exec -it sql-test-postgres psql -U admin -d employee_db",
    "check-ports": "netstat -tulpn | grep ':543'"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "sql",
    "postgresql",
    "docker",
    "database",
    "testing"
  ],
  "author": "",
  "license": "MIT"
}
